<Element
  element="mHome"
  width="25%"
  height="100%"
  orientation="Vertical"
  backgroundColor="#54b5e0"
  alignItemsHor="Start"
  alignItemsVer="SpaceBetween"
  >
  <Element
    element="mSelection"
    width="100%"
    orientation="Vertical">
    <Element
      width="100%"
      height="6dp"
      backgroundColor="glm::vec4(255)">
    </Element>
    <Element
      width="32"
      height="16"
      borderType="Cut"
      borderRadius="glm::vec4(0, 0, 16, 16)"
      backgroundColor="glm::vec4(255)">
    </Element>
  </Element>
  <Text
    element="mTitle"
    text="HOME"
    fontSize="16dp"
    textColor="#29216c"
    fontStyle="Bold"
    margin="5dp">
  </Text>

  <BadgeNew>
  </BadgeNew>

</Element>